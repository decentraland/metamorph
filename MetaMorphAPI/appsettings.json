{
  "MetaMorph": {
    "LocalWorker": false,
    "LocalCache": false,
    "StartLocalInfra": false,
    "MaxDownloadFileSize": 20971520
  },
  "AWS": {
    "ServiceURL": "",
    "S3ForcePathStyle": false
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
        "Microsoft.AspNetCore.DataProtection": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "AllowedHosts": "*"
}
